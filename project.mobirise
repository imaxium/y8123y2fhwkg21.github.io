          "_customHTML": "<section data-bs-version=\"5.1\" class=\"mbr-section\" group=\"Dentaura Preloader\" plugins=\"Dentaurapreloader\" once=\"Dentaura Preloader\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"color\" title=\"Background Color\" name=\"preBgcolor\" value=\"#000000\">\n        <select title=\"Color\" name=\"showLoadercolor\">\n            <option value=\"singleColor\" selected>Single</option>\n\t        <option value=\"multiColor\">Multi color</option>\n        </select>\n        <input type=\"color\" title=\"Spin Color\" name=\"preLoadcolor\" value=\"#ffffff\" condition=\" showLoadercolor == 'singleColor' && showLoadercolor !== 'multiColor'\">\n        <input type=\"color\" title=\"Spin Color 1\" name=\"preLoadcolorone\" value=\"#f7ed4a\" condition=\" showLoadercolor == 'multiColor' && showLoadercolor !== 'singleColor'\">\n        <input type=\"color\" title=\"Spin Color 2\" name=\"preLoadcolortwo\" value=\"#01b64b\" condition=\" showLoadercolor == 'multiColor' && showLoadercolor !== 'singleColor'\">\n        <input type=\"color\" title=\"Spin Color 3\" name=\"preLoadcolorthree\" value=\"#bd3737\" condition=\" showLoadercolor == 'multiColor' && showLoadercolor !== 'singleColor'\">\n        <input type=\"range\" inline title=\"Spin Thickness\" name=\"spinThickness\" min=\"1\" max=\"6\" step=\"1\" value=\"3\">\n        <input type=\"checkbox\" title=\"Enable Logo\" name=\"showLogo\" checked>\n        <input type=\"range\" inline title=\"Image border round\" name=\"borderRadius\" min=\"0\" max=\"50\" step=\"1\" value=\"50\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n    <!-- Dentaura Preloader -->\n        <div id=\"loading-preloader\">\n            <div id=\"loading-center-preloader\">\n                <div id=\"loading-center-absolute\">\n                    <div class=\"spinner\">\n                        <div id=\"dentaura-spinner\"></div>\n<img src=\"@PROJECT_PATH@